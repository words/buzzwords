{
  "name": "buzzwords",
  "version": "1.0.1",
  "description": "List of (possible) English buzzwords",
  "license": "MIT",
  "keywords": [
    "buzzword",
    "word",
    "list"
  ],
  "files": [
    "index.js",
    "data/index.json"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/wooorm/buzzwords.git"
  },
  "bugs": "https://github.com/wooorm/buzzwords/issues",
  "author": "Titus Wormer <tituswormer@gmail.com> (http://wooorm.com)",
  "dependencies": {},
  "devDependencies": {
    "browserify": "^13.0.0",
    "eslint": "^3.1.1",
    "esmangle": "^1.0.0",
    "istanbul": "^0.4.0",
    "jscs": "^3.0.7",
    "jscs-jsdoc": "^2.0.0",
    "remark": "^5.0.1",
    "remark-comment-config": "^4.0.1",
    "remark-github": "^5.1.1",
    "remark-lint": "^4.0.2",
    "remark-slug": "^4.2.0",
    "remark-validate-links": "^4.0.0",
    "scraperjs": "^1.2.0",
    "tape": "^4.0.0"
  },
  "scripts": {
    "build-md": "remark . --quiet --frail",
    "build-data": "node script/build-data.js",
    "build-support": "node script/build-support.js",
    "build-bundle": "browserify index.js --bare -s buzzwords > buzzwords.js",
    "build-mangle": "esmangle buzzwords.js > buzzwords.min.js",
    "build": "npm run build-md && npm run build-support && npm run build-bundle && npm run build-mangle",
    "lint-api": "eslint .",
    "lint-style": "jscs --reporter inline .",
    "lint": "npm run lint-api && npm run lint-style",
    "test-api": "node test.js",
    "test-coverage": "istanbul cover test.js",
    "test": "npm run build && npm run lint && npm run test-coverage"
  }
}
