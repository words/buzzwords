{
  "name": "buzzwords",
  "version": "1.0.2",
  "description": "List of (possible) English buzzwords",
  "license": "MIT",
  "keywords": [
    "buzzword",
    "word",
    "list"
  ],
  "files": [
    "index.js",
    "index.json"
  ],
  "repository": "https://github.com/wooorm/buzzwords",
  "bugs": "https://github.com/wooorm/buzzwords/issues",
  "author": "Titus Wormer <tituswormer@gmail.com> (http://wooorm.com)",
  "contributors": [
    "Titus Wormer <tituswormer@gmail.com> (http://wooorm.com)"
  ],
  "dependencies": {},
  "devDependencies": {
    "browserify": "^13.0.1",
    "esmangle": "^1.0.1",
    "nyc": "^9.0.1",
    "remark-cli": "^2.0.0",
    "remark-preset-wooorm": "^1.0.0",
    "scraperjs": "^1.2.0",
    "tape": "^4.0.0",
    "xo": "^0.17.0"
  },
  "scripts": {
    "build-md": "remark . --quiet --frail",
    "build-generate": "node script/generate.js",
    "build-bundle": "browserify index.js --bare -s buzzwords > buzzwords.js",
    "build-mangle": "esmangle buzzwords.js > buzzwords.min.js",
    "build": "npm run build-md && npm run build-bundle && npm run build-mangle",
    "lint": "xo",
    "test-api": "node test",
    "test-coverage": "nyc --reporter lcov tape test.js",
    "test": "npm run build && npm run lint && npm run test-coverage"
  },
  "xo": {
    "space": true,
    "ignores": [
      "buzzwords.js"
    ]
  },
  "remarkConfig": {
    "output": true,
    "presets": "wooorm"
  }
}
